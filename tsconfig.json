{
    "compilerOptions": {
        "target": "esnext",
        "lib": ["esnext"],
        "esModuleInterop": true,
        "isolatedModules": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "noEmit": true,
        "incremental": true
    }
}
